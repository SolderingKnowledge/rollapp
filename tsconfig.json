{
	"compilerOptions": {
		"target": "esnext",
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"baseUrl": "src",
		"typeRoots": ["node_modules/@types"]
	},
	"include": ["src"],
	"exclude": []
}
